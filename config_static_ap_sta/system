
config system
	option hostname 'JoySince'
	option timezone 'UTC'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enabled '1'
	option enable_server '0'

config led 'led_lan'
	option name 'LAN'
	option sysfs 'tp-link:blue:eth1'
	option trigger 'netdev'
	option dev 'eth1'
	option mode 'link tx rx'
	option default '0'

config led 'led_wan'
	option name 'WAN'
	option sysfs 'tp-link:blue:eth0'
	option trigger 'netdev'
	option dev 'eth0'
	option mode 'link tx rx'
	option default '0'

config led 'led_wlan'
	option name 'WLAN'
	option sysfs 'tp-link:blue:wlan'
	option trigger 'netdev'
	option dev 'wlan0'
	option mode 'link tx rx'
	option default '0'

config led 'led_system'
	option name 'SYSTEM'
	option sysfs 'tp-link:blue:system'
	option trigger 'timer'
	option default '0'
	option delayon '1000'
	option delayoff '1000'

config button
	option button 'BTN_0'
	option action 'released'
	option handler 'reboot'
	option min '0'
	option max '3'

config button
	option button 'BTN_0'
	option action 'released'
	option handler 'jffs2reset -y && reboot'
	option min '5'
	option max '30'

config led
	option default '0'
	option name 'blue'
	option sysfs 'ath9k-phy0'
	option trigger 'timer'
	option delayon '2000'
	option delayoff '2000'

config button
	option button 'BTN_0'
	option action 'released'
	option handler 'reboot'
	option min '0'
	option max '3'

